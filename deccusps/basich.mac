	TLNN	C,F.RPRN	;MUST HAVE PARENTHESIS
	JRST	ERRPRN
	JRST	NXCHK		;IT DOES. SKIP PAREN AND RETURN.


;ROUTINE TO GEN ARGUMENTS

GENARG:	JUMPE	B,GENAFN	;ONE OR TWO ARGS?
GENAR0:	PUSHJ	P,POPPR		;TWO
	PUSHJ	P,EXCHG

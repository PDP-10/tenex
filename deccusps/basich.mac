	HLLZM	1,FILNM+1
	PUSHJ	P,CLSTU3	;OUTPUT FILENAME AND EXT.
CATTST:	SOSG	CATFLG		;ONLY 22 FILES ON A DECTAPE.
	JRST	UXIT
	JRST	CATLP

DSKHAN:	SKIPL	DEVBAS		;FAKED DEVICE BAS?
	JRST	DSKH0
	MOVE	T1,[XWD 5,1]	;YES.
	JRST	DSKH1
DSKH0:	MOVE	T1,DEVICE	;NO. PREPARE FOR LOOKUP.
	DEVPPN	T1,
	JRST	[MOVE T1,DEVICE
		MOVE N,T
		CAMN T1,[SIXBIT/SYS/]
		SKIPA T1,[XWD 1,4]
		GETPPN	T1,
		CAMN N,[SIXBIT/BAS/]
		MOVE T1,[XWD 5,1]

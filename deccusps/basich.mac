	TLCE	T1, 1000		;SUBTRACT 1.0  FROM ARG IF BITS ARE
	FSBRI	T,201400		;01 OR 11
	TLCE	T1, 3000		;CHECK FOR FIRST QUADRANT, 01
	TLNN	T1, 3000		;CHECK FOR THIRD QUADRANT, 10

	SKIPL NXTDMP		;DDMP FINISHED?
	 JRST EHLT1		;NO
	MOVSI 1,(1B0)
	MOVEM 1,20		;HALT SCHED
	JRST EXCRR

EAUTO:	MOVEI 1,100
	MOVEI 3,12
	NIN
	 JRST [	SETO 2,
		JRST EAUTO1]
	CAIL 2,0
	CAILE 2,NPACKS-1
	 JRST EXERR
EAUTO1:	MOVEI 1,100
	BKJFN
	 JRST EXERR
	PBIN
	CAIE 1,37
	 JRST EXERR
	SETZM NXTDMP		;REQUEST DDMP ACTION
	SETZM DDTIME		;FORCE DDMP RUN
EAUTO2:	AOS JB0FLG		;REQUEST JOB 0
	MOVEI 1,^D1000
	DISMS			;WAIT A SECOND
	SKIPL NXTDMP		;DDMP FINISHED?
	 JRST EAUTO2		;NO
	MOVSI 1,(1B3)

	SEARCH	PROLOG
	TITLE	SYSNAM
	SWAPCD

;SYSTEM VERSION TEXT
; COMPUTE VERSION NUMBERS

%RV=SVNM/^D100		; SEPARATE EDIT VERSION FROM REST
%EV=SVNM-%RV*^D100	; EDIT VERSION
%SV=%RV/^D100		; AND SYSTEM VERSION FROM RELEASE VERSION
%RV=%RV-%SV*^D100

DEFINE	%M(%S,%R,%E)<	;; MACRO TO EXPAND VERSION INTO STRINGS
			;; SYSNAM DEFINED IN PARAMS
	SYSNAM(<ASCIZ \PARC-MAXC TENEX %S'.'%R'.'%E, >,<\>)
>

RADIX ^D10

SVN:	%M(\%SV,\%RV,\%EV)
	0			;FOR PATCHES
SVNN==.-SVN
SVNUM:	SVNM
INTERN SVNN,SVN,SVNUM
PURGE	%SV,%RV,%EV,%M

	END

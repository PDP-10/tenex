.TITLE PUPDG 

.CSECT GLOBAL
G=.

.CSECT

; ENTRY POINT TO INITFORWARDER
LA1:
	SUB @(6),%0
	MOV %7,@%0
	MOV (6)+,-(0)
	MOV %0,%5
	MOV %1,-(5)
	MOV -2.(0),-4.(0)
	MOV (0)+,%5
	ADD (5)+,%0
	JMP @%5


; ENTRY POINT TO FORWARDER
LA2:
	SUB @(6),%0
	MOV %7,@%0
	MOV (6)+,-(0)
	MOV %0,%5
	MOV %1,-(5)
	MOV -2.(0),%1
	JSR %7,@G+306.
	000004
	MOV (0)+,%5
	ADD (5)+,%0
	JMP @%5


; ENTRY POINT TO FORWARDERCTX
LA3:
	SUB @(6),%0
	MOV %7,@%0
	MOV (6)+,-(0)
	MOV #000001,%1
	JSR %7,@G+248.
	000002
	MOV (0)+,%5
	ADD (5)+,%0
	JMP @%5

LA4:

.CSECT GLOBAL

	.=G+332.
	LA1
	.=G+334.
	LA2
	.=G+336.
	LA3

.CSECT



.END
